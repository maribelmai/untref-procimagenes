<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ar.edu.untref.procesamientoimagenes">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".Aplicacion"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".actividad.ActividadPrincipal"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".actividad.ActividadObtenerPixel"
            android:screenOrientation="landscape"/>

        <activity android:name=".actividad.ActividadDegrade"
            android:screenOrientation="landscape"/>

        <activity android:name=".actividad.ActividadRecortar"
            android:screenOrientation="landscape"/>

        <activity android:name=".actividad.ActividadFormas"
            android:screenOrientation="landscape"/>

        <activity android:name=".actividad.ActividadOperaciones"
            android:screenOrientation="landscape"/>

        <activity android:name=".actividad.ActividadAumentarContraste"
            android:screenOrientation="landscape"/>

        <activity android:name=".actividad.ActividadHistograma"
            android:screenOrientation="landscape"/>

        <activity android:name=".actividad.ActividadUmbral"
        android:screenOrientation="landscape"/>

        <activity android:name=".actividad.ActividadRuidos"
            android:screenOrientation="landscape"/>

        <activity android:name=".actividad.ActividadFiltros"
            android:screenOrientation="landscape"/>

        <activity android:name=".actividad.ActividadEcualizar"
            android:screenOrientation="landscape"/>

        <activity android:name=".actividad.ActividadBordes"
            android:screenOrientation="landscape"/>

    </application>

</manifest>